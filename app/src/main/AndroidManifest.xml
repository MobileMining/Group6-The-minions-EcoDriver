            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TrackDriving"
            android:label="@string/title_activity_track_driving" >
        </activity>
        <activity
            android:name=".AnalyzeDrives"
            android:label="@string/title_activity_analyze_drives" >
        </activity>
        <activity
            android:name=".DriveDetails"
            android:label="DriveDetails">
